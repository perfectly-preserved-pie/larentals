{
  "agent": {
    "metrics_collection_interval": 60,
    "run_as_user": "root"
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "~/log/larentals/**/*.log",
            "log_group_name": "larentals-logs",
            "log_stream_name": "{instance_id}/{file}",
            "timestamp_format": "%Y-%m-%d %H:%M:%S %Z"
          }
        ]
      }
    }
  }
}